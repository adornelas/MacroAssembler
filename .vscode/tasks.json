{
  "tasks": [
    {
      "label": "Build project",
      "windows": {
        "command": "mingw32-make.exe"
      },
      "linux": {
        "command": "/usr/bin/make"
      },
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": ["$gcc"],
      "group": "build"
    }
  ],
  "version": "2.0.0"
}
